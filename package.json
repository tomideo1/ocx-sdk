{
  "name": "ocxsdk",
  "version": "1.0.0",
  "description": "octopus x sdk to connect to the octopusx grids and services",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "precommit": "pretty-quick --staged"
  },
  "author": "TomideAina",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.161",
    "husky": "^0.14.3",
    "jest": "^26.5.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "typescript": "^3.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./preprocessor.js"
    },
    "testMatch": [
      "**/tests/*.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "axios": "^0.20.0",
    "lodash": "^4.17.20",
    "object-assign-deep": "^0.3.1"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 200,
    "semi": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
